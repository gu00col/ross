{
    "name": "ross/analista-juridico",
    "description": "Sistema de An√°lise Contratual Automatizada - ROSS",
    "type": "project",
    "require": {
        "php": "^8.2",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.0",
        "twig/twig": "^3.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/console": "^6.0",
        "doctrine/orm": "^2.15",
        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "^3.6",
        "ramsey/uuid": "^4.7",
        "firebase/php-jwt": "^6.8",
        "phpmailer/phpmailer": "^6.8",
        "league/container": "^4.2",
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^2.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.19"
    },
    "authors": [
        {
            "name": "Luis Oliveira",
            "email": "luis@ross.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "MF\\": "vendor/MF/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs --standard=PSR12 public_html/app/",
        "cs-fix": "phpcbf --standard=PSR12 public_html/app/",
        "stan": "phpstan analyse public_html/app/",
        "migrate": "php public_html/app/console.php migrations:migrate",
        "serve": "php -S localhost:8000 -t public_html/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
